<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="populate-users-table" author="ykuzmich">
        <loadData tableName="users" file="data/changelog/0.0.1/seed/users.csv" schemaName="public">
            <column name="id" type="numeric"/>
            <column name="email" type="string"/>
            <column name="hash_password" type="string"/>
            <column name="user_type_id" type="numeric"/>
            <column name="user_status_id" type="numeric"/>
        </loadData>
    </changeSet>
</databaseChangeLog>
